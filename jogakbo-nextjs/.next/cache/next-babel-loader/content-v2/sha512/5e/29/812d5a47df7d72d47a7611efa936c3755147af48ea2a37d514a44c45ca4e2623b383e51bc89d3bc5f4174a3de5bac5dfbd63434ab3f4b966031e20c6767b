: border-box;\n  ${media.tablet} {\n    &:first-child {\n      margin-bottom: 100px;\n    }\n  }\n  ${media.mobile} {\n    padding: 0 20px;\n  }\n`;\nconst Title = styled.h3`\n  font-size: 22px;\n  margin-bottom: 10px;\n`;\n\ninterface IGridBox {\n  type: string;\n}\n\nconst GridBox = styled.div<IGridBox>`\n  display: grid;\n  gap: 5px;\n  grid-template-columns: repeat(\n    ${(props) => (props.type === \"All\" ? 5 : 3)},\n    1fr\n  );\n  ${media.mobile} {\n    grid-template-columns: repeat(\n      ${(props) => (props.type === \"All\" ? 3 : 1)},\n      1fr\n    );\n  }\n`;\n\nconst Box = styled.div`\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  height: 200px;\n  background: lightgray;\n  border-radius: 10px;\n  transition: all 0.2s ease-in-out;\n  &:hover {\n    background: ${(props) => props.theme.gradient};\n    opacity: 0.5;\n  }\n`;\n\nconst Slider = (props: any) => {\n  const type = props.type;\n  const [data, setData] = useState<any>([]);\n\n  function matchData() {\n    if (type === \"New\") {\n      const newData = [...props.data];\n      const results = newData.reverse().slice(0, 3);\n      setData(results);\n    } else if (type === \"Popular\") {\n      const popularData = [...props.data];\n      const results = popularData.sort(\n        (b: any, a: any) => b[\"targetAmount\"] - a[\"targetAmount\"]\n      );\n      results.reverse();\n      setData(results.slice(0, 3));\n    } else if (type === \"All\") {\n      setData(props.data);\n    }\n  }\n\n  useEffect(() => {\n    matchData();\n  }, []);\n\n  return (\n    <Container>\n      <Title>{type} Campaigns</Title>\n      <GridBox type={type}>\n        {data.map((data: any, i: number) => (\n          <Link href={`campaigns/${data.name}/${data.id}`} key={data.id}>\n            <a>\n              <Box>\n                <span>{data.id}</span>\n                <h3>{data.name}</h3>\n                <p>{data.description}</p>\n              </Box>\n          